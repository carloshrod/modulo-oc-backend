{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "standard",
    "eslint-config-prettier"
  ],
  "parser": "espree",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [],
  "rules": {
    "no-unused-vars": "warn",
    "space-before-function-paren": "off",
    "camelcase": "off"
  }
}